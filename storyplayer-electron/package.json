{
  "name": "storyplayer-electron",
  "productName": "storyplayer-electron",
  "version": "0.1.2",
  "description": "My Electron application description",
  "main": "src/index.js",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "compile": "yarn build  && electron ./dist/src/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:all": "electron-builder -mwl"
  },
  "postinstall": "electron-builder install-app-deps",
  "build": {
    "productName": "StoryPlayer",
    "appId": "com.storyplayer",
    "mac": {
      "category": "public.app-category.video",
      "identity": null
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "esm": "^3.2.25"
  },
  "dependencies": {
    "@bbc/rd-ux-media-resolver": "^3.0.5"
  }
}
