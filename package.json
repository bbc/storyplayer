{
  "name": "@bbc/romper",
  "version": "0.2.39",
  "description": "Object Based Media player",
  "main": "dist/romper.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "webpack",
    "test": "flow-typed install && concurrently --names \"eslint,flow,jest,sass-lint\" -c \"blue.bgWhite,cyan.bgWhite,magenta.bgWhite,black.bgWhite\" --prefix \"[{name}]\" \"yarn test:eslint\" \"yarn test:flow\" \"yarn test:jest\" \"yarn test:sass-lint\"",
    "test-noinstall": "concurrently --names \"eslint,flow,jest,sass-lint\" -c \"blue.bgWhite,cyan.bgWhite,magenta.bgWhite,black.bgWhite\" --prefix \"[{name}]\" \"yarn test:eslint\" \"yarn test:flow\" \"yarn test:jest\" \"yarn test:sass-lint\"",
    "test-watch": "flow-typed install && nodemon -L --watch src --exec yarn test-noinstall",
    "test:eslint": "eslint src/ spec/",
    "test:flow": "flow check",
    "test:jest": "jest",
    "test:sass-lint": "sass-lint -v",
    "demo": "yarn build && rm -f ./tmp/*.json && node demos.js",
    "watch": "concurrently \"webpack --watch --watch-poll 500 --progress --profile --bail\" \"jest --watch\" \"nodemon --watch src --watch spec --exec node_modules/.bin/eslint src/ spec/\" \"nodemon --watch src --watch spec --exec 'node_modules/.bin/flow check --all || exit 1'\"",
    "dev": "webpack --watch --watch-poll 500 --progress --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbc/romper.git"
  },
  "author": "BBC R&D",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bbc/romper/issues"
  },
  "homepage": "https://github.com/bbc/romper#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.94.0",
    "flow-typed": "^2.5.1",
    "jest-cli": "^24.1.0",
    "jest-junit-reporter": "^1.1.0",
    "jsoneditor": "^5.17.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.7.2",
    "nodemon": "1.18.10",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "aframe": "^0.9.0",
    "browser-bunyan": "^1.2.2",
    "dashjs": "^2.6.7",
    "express": "^4.16.3",
    "hls.js": "^0.12.2",
    "json-logic-js": "^1.0.11",
    "uuid": "^3.3.2"
  },
  "resolutions": {
    "aframe/**/browserify-css": "^0.14.0"
  }
}
