{
  "name": "@bbc/storyplayer",
  "version": "1.2.0",
  "description": "StoryPlayer - reference player for BBC Research & Development's object-based media schema (https://www.npmjs.com/package/@bbc/object-based-media-schema)",
  "main": "./dist/storyplayer.js",
  "module": "./dist/storyplayer.js",
  "ttttttttype": "module",
  "keywords": [
    "BBC",
    "BBC Research & Development",
    "player",
    "interactive",
    "media",
    "object",
    "personalised"
  ],
  "scripts": {
    "build": "vite build",
    "test": "concurrently --names \"eslint,sass-lint,tsc\" -c \"blue.bgWhite,magenta.bgWhite,green.bgBlack\" --prefix \"[{name}]\" \"yarn test:eslint\" \"yarn test:sass-lint\" \"yarn test:typescript\"",
    "test:eslint": "eslint src/",
    "test:sass-lint": "sass-lint -v",
    "test:typescript": "tsc",
    "prepublishOnly": "yarn run test && yarn run build",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbc/storyplayer.git"
  },
  "author": "BBC R&D",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/bbc/storyplayer/issues"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/bbc/storyplayer#readme",
  "devDependencies": {
    "@bbc/object-based-media-schema": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "jsoneditor": "^9.9.0",
    "sass": "^1.53.0",
    "sass-lint": "^1.13.1",
    "typescript": "^4.9.4",
    "utility-types": "^3.10.0",
    "vite": "^4.0.3",
    "vite-plugin-flow": "^0.2.1"
  },
  "dependencies": {
    "@types/node": "^18.11.18",
    "browser-bunyan": "^1.5.3",
    "events": "^3.3.0",
    "hls.js": "^1.2.9",
    "json-logic-js": "^2.0.0",
    "shaka-player": "3.0.7",
    "three": "^0.148.0",
    "uuid": "^9.0.0"
  }
}
